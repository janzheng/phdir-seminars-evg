{
  "version": 2,
  "name": "evergreen-phage-directory",
  "builds": [
    {
      "src": "package.json",
      "use": "now-sapper"
    }
  ],
  "headers": [
    {
      "source": "/client/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/submit",
      "destination": "https://docs.google.com/forms/d/e/1FAIpQLSdLVcsoc198G1iVuBh1ToVc5x7UNyDNuuaPcnF2cd4h8-4e5w/viewform"
    }
  ]
}